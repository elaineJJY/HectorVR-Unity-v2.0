<?xml version="1.0"?>

<launch>
  
  <arg name="add_default_geom" default="true"/>
  <arg name="add_containtment_self_filter_geom" default="false"/>
  <arg name="add_raycast_self_filter_geom" default="false"/>
  
  <include file="$(find drz_telemax_description)/launch/xacrodisplay_common.launch">
    <!--Display default robot model-->  
    <arg name="robot_description" value="$(find xacro)/xacro $(find drz_telemax_description)/urdf/drz_telemax_complete.urdf.xacro add_default_geom:=$(arg add_default_geom) add_containment_self_filter_geom:=$(arg add_containtment_self_filter_geom) add_raycast_self_filter_geom:=$(arg add_raycast_self_filter_geom)"/>    
  </include>  
  
</launch>
